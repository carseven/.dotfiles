    "when": "editorTextFocus"
  },
  {
    "key": "alt+left",
    "command": "workbench.action.navigateBack",
    "when": "!isMac"
  },
  {
    "key": "alt+right",
    "command": "workbench.action.navigateForward",
    "when": "!isMac"
  },
  // TODO: Fix go back and forward with macos. Choose another keybinding
  {
    "key": "ctrl+left",
    "command": "workbench.action.navigateBack",
    "when": "isMac"
  },
  {
    "key": "ctrl+right",
    "command": "workbench.action.navigateForward",
    "when": "isMac"
  },
  {
