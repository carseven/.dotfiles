  },
  {
    "comment": "Kill terminal",
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "isMac && (terminalFocus && terminalProcessSupported && !terminalEditorFocus)"
  },
  {
    "comment": "Kill terminal instance",
