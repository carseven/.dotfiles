{
  "global": {
    "check_for_updates_on_startup": true,
    "show_in_menu_bar": true,
    "show_profile_name_in_menu_bar": false
  },
  "profiles": [
    {
      "complex_modifications": {
        "parameters": {
          "basic.simultaneous_threshold_milliseconds": 50,
          "basic.to_delayed_action_delay_milliseconds": 500,
          "basic.to_if_alone_timeout_milliseconds": 1000,
          "basic.to_if_held_down_threshold_milliseconds": 500,
          "mouse_motion_to_scroll.speed": 100
        },
        "rules": [
          {
            "manipulators": [
              {
                "description": "Change caps_lock to control+option+shift (HYPER)",
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Open Browser by left_option+b",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "b",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Brave Browser.app'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Terminal by left_option+t",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "t",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Ghostty.app"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Notes by left_option+n",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "n",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Visual Studio Code.app' /Users/hpbuild/personal/notes"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Visual Studio Code by left_option+c",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "c",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
