                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Visual Studio Code.app'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Postman by left_option+p",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "p",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Postman.app"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Microsoft Teams by left_option+m",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "m",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Microsoft Teams.app'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Microsoft Outlook by left_option+e",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "e",
                  "modifiers": {
                    "mandatory": [
                      "left_option"
                    ],
                    "optional": [
                      "caps_lock"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Microsoft Outlook.app'"
                  }
                ]
              }
            ]
          }
        ]
      },
      "devices": [],
      "fn_function_keys": [],
      "name": "Default profile",
      "parameters": {
        "delay_milliseconds_before_open_device": 1000
      },
      "selected": true,
      "simple_modifications": [],
      "virtual_hid_keyboard": {
        "country_code": 0,
        "indicate_sticky_modifier_keys_state": true,
        "mouse_key_xy_scale": 100
      }
    }
  ]
